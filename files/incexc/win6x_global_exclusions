# Error reports
#exclude_regex="C:/Users/[^/]+/AppData/Local/Microsoft/Windows/WER"
# Windows 10 Edge cache
#exclude_regex="C:/Users/[^/]+/AppData/Local/Microsoft/Windows/InetCache"
#exclude_regex="C:/Users/[^/]+/AppData/Local/Microsoft/Windows/WebCache"
#exclude_regex="C:/Users/[^/]+/AppData/Local/Microsoft/Windows/AppCache"
#exclude_regex="C:/Users/[^/]+/AppData/Roaming/Microsoft/Windows/Cookies"
#exclude_regex=".*/MSOCache.*"
