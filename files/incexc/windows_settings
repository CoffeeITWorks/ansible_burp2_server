# Generic excludes
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Temp
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/LocalLow
exclude_regex = ^[A-Z]:/Documents and Settings/[^/]+/Cookies
exclude_regex = ^[A-Z]:/Documents and Settings/[^/]+/Recent
exclude_regex = ^[A-Z]:/Documents and Settings/[^/]+/Local Settings/Temp
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Temp
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/History
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Application Data
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Temporary Internet Files
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Microsoft/Internet Explorer
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Microsoft/Windows/Temporary Internet Files
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Microsoft/Windows/Caches
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Microsoft/Terminal Server Client/Cache
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/[^/]+/Temp
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Roaming/Microsoft/Windows/Themes/CachedFiles
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/[^/]+/Internet Explorer
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/[^/]+/Microsoft/[^/]+/[^/]*[Cc]ache
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/[^/]+/Microsoft/[^/]+/InetCookies
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/[^/]+/Microsoft Office/[^/]+OfficeFileCache
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Microsoft/Outlook/RoamCache
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Microsoft/Office/[0-9\.]+/WebServiceCache
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/LocalLow/Microsoft/CryptnetUrlCache
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Downloaded Installations

# Various Win 10 caches
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Microsoft/Windows/Caches
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Microsoft/Windows/ActionCenterCache/
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Microsoft/Feeds Cache

# Error reports
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Microsoft/Windows/WER
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/CrashDumps
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Diagnostics

# Windows 10 Edge
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Microsoft/Windows/[^/]*[Cc]ache
exclude_regex = ^[A-Z]:/Users/[^/]+/MicrosoftEdgeBackups/backups
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/MicrosoftEdge/SharedCacheContainers

# Windows 10 Store Application cache and state
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Packages/[^/]+/AC
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Packages/[^/]+/TempState
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Packages/[^/]+/LocalState
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Packages/[^/]+/LocalCache
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Packages/[^/]+/RoamingState
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Packages/[^/]+/AppData/User/Default/CacheStorage

# Windows 10 various stuff
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Microsoft/Windows/Notifications
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Microsoft/Windows/Explorer

# Unnecessary folder exclusions
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Roaming/Microsoft/Windows/Cookies
exclude_regex = ^[A-Z]:/Users/[^/]+/NetHood
exclude_regex = ^[A-Z]:/Users/[^/]+/PrintHood
exclude_regex = ^[A-Z]:/Users/[^/]+/Cookies
exclude_regex = ^[A-Z]:/Users/[^/]+/Recent
exclude_regex = ^[A-Z]:/Users/[^/]+/SendTo
exclude_regex = ^[A-Z]:/Users/[^/]+/LocalService
exclude_regex = ^[A-Z]:/Users/[^/]+/NetworkService

# Generic system file exlusions
exclude_regex = ^[A-Z]:/*/MSOCache
exclude_regex = ^[A-Z]:/*/Config\.Msi

# Chrome 66+

exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Google/Chrome/User Data/Default/[^/]*[Cc]ache
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Google/Chrome/User Data/ShaderCache
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Google/Chrome/User Data/Default/Session Storage
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Google/Chrome/User Data/Default/Local Storage
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Google/Chrome/User Data/Default/Extensions/Temp
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Google/Chrome/User Data/Default/Service Worker/CacheStorage
# Chrome can also store in Roaming instead of Local
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Roaming/Google/Chrome/User Data/Default/[^/]*[Cc]ache
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Roaming/Google/Chrome/User Data/ShaderCache
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Roaming/Google/Chrome/User Data/Default/Session Storage
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Roaming/Google/Chrome/User Data/Default/Local Storage
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Roaming/Google/Chrome/User Data/Default/Extensions/Temp
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Roaming/Google/Chrome/User Data/Default/Service Worker/CacheStorage

# Opera 41+
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Opera/Opera/profile/cache4
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Opera Software/Opera [^/]+/[^/]*[Cc]ache
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Roaming/Opera Software/Opera [^/]+/Local Storage
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Roaming/Opera Software/Opera [^/]+/Crash Reports
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Roaming/Opera Software/Opera [^/]+/[^/]*[Cc]ache
# Firefox 44+
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Mozilla/Firefox/Profiles/[^/]+/[^/]*[Cc]ache
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Mozilla/Firefox/Profiles/[^/]+/[Tt]humbnails
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Roaming/Mozilla/Firefox/Crash Reports

# Thunderbird
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Thunderbird/Mozilla Thunderbird/updates
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Thunderbird/Profiles/[^/]+/[Cc]ache[^/]*
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Roaming/Thunderbird/Crash Reports
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Roaming/Thunderbird/Profiles/[^/]+/crashes

# Google Apps Sync
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Google/Google Apps Sync/Tracing

# Adobe Acrobat DC
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/Adobe/AcroCef/DC/Acrobat/Cache

# Apple iPhone backups :(
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Roaming/Apple Computer/MobileSync/Backup

# Java
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/LocalLow/Sun/Java/Deployment/cache

# Cisco Webex
exclude_regex = ^[A-Z]:/Users/[^/]+/AppData/Local/WebEx/wbxcache

# Dropbox, OneDrive, SkyDrive (not excluded by default because of cryptolockers attacks)
#exclude_regex = ^[A-Z]:/Users/[^/]+/SkyDrive[^/]+
#exclude_regex = ^[A-Z]:/Users/[^/]+/Dropbox
#exclude_regex = ^[A-Z]:/Users/[^/]+/OneDrive

# Windows downloads
exclude_regex = ^[A-Z]:/Users/[^/]+/Downloads

# Windows update cache
exclude_regex = ^[A-Z]:/Windows/SoftwareDistribution/Download

exclude_regex = ^[A-Z]:/Windows/Temp
exclude_regex = ^[A-Z]:/Windows/Downloaded Program Files
exclude_regex = ^[A-Z]:/RECYCLER$
exclude_regex = ^[A-Z]:/\$recycle\.bin$
exclude_regex = ^[A-Z]:/System Volume Information$
# swap file (Windows XP, 7, 8)
exclude_regex = ^[A-Z]:/pagefile\.sys$
# swap file?? (Windows 8)
exclude_regex = ^[A-Z]:/swapfile\.sys$
exclude_regex = ^[A-Z]:/hiberfil\.sys$
# Windows temp installer
exclude_regex = ^[A-Z]:/\$WINDOWS\.~BT$
# Windows 10 Upgrade previous install
exclude_regex = ^[A-Z]:/Windows\.old$
exclude_regex = ^[A-Z]:/PerfLogs$


# Windows filesystem directories
exclude_regex = ^[A-Z]:/\$mft$
exclude_regex = ^[A-Z]:/\$logfile$
exclude_regex = ^[A-Z]:/\$volume$
exclude_regex = ^[A-Z]:/\$bitmap$
exclude_regex = ^[A-Z]:/\$extend$
exclude_regex = ^[A-Z]:/\$reparse$
